<?xml version="1.0" encoding="ISO-8859-1"?>

<process-definition xmlns="urn:jbpm.org:jpdl-3.2" name="Fluxo de Notificação - INSS"> 
    <!-- SWIMLANES -->
    <swimlane name="Nó de Desvio - Fluxo de Notificação - INSS">
        <assignment pooled-actors="#{localizacaoAssignment.getPooledActors('11:1,1229:1')}"/>
    </swimlane>
    <swimlane name="solicitante">
        <assignment pooled-actors="#{localizacaoAssignment.getPooledActors('1230:1469,5:1469,1239:5197,1239:5533,8:5197,8:5533,11:1338,11:5532,1229:1338,1229:5532,11:5607,1229:5607')}"/>
    </swimlane>  
    <!-- START-STATE -->
    <start-state name="Início">
        <task name="Tarefa inicial" swimlane="solicitante" priority="3"/>
        <transition to="[INSS] Notificação recebida" name="[INSS] Notificação recebida"/>
    </start-state>  
    <!-- NODES -->
    <task-node end-tasks="true" name="[INSS] Notificação recebida">
        <task name="[INSS] Notificação recebida" swimlane="solicitante" priority="3">
            <controller>
                <variable name="AvisoNotificacao" mapped-name="textMessage:AvisoNotificacao" access="read,write"/>
                <variable name="MovimentarEmLote" mapped-name="movimentarLote:MovimentarEmLote" access="read,write"/>
            </controller>
        </task>
        <transition to="Término" name="Finalizar"/>
        <transition to="Nó de Desvio - Fluxo de Notificação - INSS" name="Nó de Desvio - Fluxo de Notificação - INSS">
            <condition expression="#{true}"/>
        </transition>
        <event type="node-enter">
            <action name="upd" expression="#{preencherMovimento.deCodigo(132).lancarMovimento()}"/>
        </event>
    </task-node>
    <end-state name="Término"/>
    <task-node end-tasks="true" name="Nó de Desvio - Fluxo de Notificação - INSS">
        <task name="Nó de Desvio - Fluxo de Notificação - INSS" swimlane="Nó de Desvio - Fluxo de Notificação - INSS" priority="3"/>
        <transition to="Término" name="Término"/>
        <transition to="[INSS] Notificação recebida" name="[INSS] Notificação recebida"/>
    </task-node>  
    <!-- PROCESS-EVENTS -->
    <event type="superstate-enter">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="process-start">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="before-signal">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-end">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-create">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="subprocess-created">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-assign">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
